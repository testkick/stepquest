{
  "name": "@fastshot/ai",
  "version": "1.0.0",
  "description": "Newell AI SDK for Expo React Native applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest --config jest.integration.config.js",
    "test:integration:watch": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest --config jest.integration.config.js --watch",
    "test:integration:secure": "jest --config jest.integration.config.js",
    "test:integration:local": "EXPO_PUBLIC_NEWELL_API_URL=http://localhost:8003 EXPO_PUBLIC_PROJECT_ID=00a4e5a9-b161-414d-bc23-e5227d6d9b9f jest --config jest.integration.config.js"
  },
  "keywords": [
    "newell",
    "ai",
    "react-native",
    "expo",
    "typescript",
    "sdk"
  ],
  "author": "Fastshot",
  "license": "MIT",
  "peerDependencies": {
    "react": "^19.0.0",
    "react-native": ">=0.70.0"
  },
  "devDependencies": {
    "@testing-library/react-native": "^12.4.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "~19.1.10",
    "form-data": "^4.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-fetch": "^2.7.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-test-renderer": "19.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.9.2"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
